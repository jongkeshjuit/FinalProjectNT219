{
  "dashboard": {
    "id": null,
    "title": "Quantum Commerce Dashboard",
    "tags": ["quantum-commerce"],
    "timezone": "browser",
    "panels": [
      {
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "id": 1,
        "title": "Payment Success Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(payments_total{status='success'}[5m]) / rate(payments_total[5m]) * 100",
            "legendFormat": "Success Rate %"
          }
        ]
      },
      {
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "id": 2,
        "title": "API Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(api_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ]
      },
      {
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "id": 3,
        "title": "Payment Amounts",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(payment_amount_usd_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ]
      },
      {
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "id": 4,
        "title": "Crypto Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(crypto_operations_total[5m])",
            "legendFormat": "{{operation_type}} - {{algorithm}}"
          }
        ]
      }
    ],
    "refresh": "10s",
    "schemaVersion": 16,
    "version": 0
  }
}
