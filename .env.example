# Quantum Commerce Environment
# Copy to .env và điền values

# Application
APP_NAME=quantum-commerce
APP_ENV=development
DEBUG=true

# SECURITY - Set these first!
MASTER_PASSWORD=your_master_password_here
VAULT_TOKEN=your_vault_token

# Database
DB_USER=quantum_user
DB_NAME=quantum_commerce
DB_HOST=localhost
DB_PORT=5432

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# Monitoring
GRAFANA_PASSWORD=quantum_admin

# Features
USE_REAL_CRYPTO=true
RATE_LIMIT_ENABLED=true

# Note: Actual passwords load từ encrypted secrets
