# .env
# NEVER commit this file to git!

# Application
APP_NAME=quantum-commerce
APP_ENV=development
APP_DEBUG=true

# Security Keys (Generate with: openssl rand -base64 32)
JWT_SECRET_KEY=your_generated_jwt_secret_here_base64_32chars
DILITHIUM_MASTER_KEY=your_generated_dilithium_key_base64_64chars  
IBE_MASTER_KEY=your_generated_ibe_key_base64_32chars
DATABASE_ENCRYPTION_KEY=your_generated_db_key_base64_32chars

# Database
DATABASE_URL=postgresql://quantum_user:quantum_pass@localhost:5432/quantum_commerce
REDIS_URL=redis://localhost:6379

# External Services
VAULT_ADDR=http://localhost:8200
VAULT_TOKEN=your_vault_token_here

# Monitoring
PROMETHEUS_METRICS_PORT=8080
GRAFANA_ADMIN_PASSWORD=your_grafana_password

# Payment Gateway (for production)
STRIPE_SECRET_KEY=sk_test_your_stripe_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret